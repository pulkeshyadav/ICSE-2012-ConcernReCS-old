/**Class for the Dedicated implementation constant code smell*/

package ufmg.crcs.smells;

public class PrimitiveConstantAttribute extends CodeSmell
{	
	private static final String NAME="Primitive constant attribute"; //The name of the code smell
	private static final String MISTAKE="Duplicated crosscutting code"; //The mistake that the smell can lead
	private static final double ERROR_PRONENESS=0.75; //Error-proneness of the code smell
	
	/**
	 * Initializes the code smell with a constant value for the name, mistake and error_proneness
	 */
	public PrimitiveConstantAttribute(String concern,String source,String where)
	{
		super(NAME,MISTAKE,concern,ERROR_PRONENESS,source,where);
	}
}
